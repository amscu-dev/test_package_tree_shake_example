{
  "name": "@amscu/test-package-tree-shakeking",
  "version": "1.0.1",
  "description": "This package implements react components based on MUI",
  "type": "module",
  "main": "lib/cjs/index.js",
  "exports": {
    "import": {
      "default": "./lib/esm/index.js",
      "types": "./lib/types/index.d.ts"
    },
    "require": {
      "default": "./lib/cjs/index.js",
      "types": "./lib/types/index.d.ts"
    }
  },
  "sideEffects": false,
  "files": [
    "lib"
  ],
  "scripts": {
    "build:rollup": "rollup -c --bundleConfigAsCjs",
    "build:tsc": "rimraf ./lib && tsc --skipLibCheck --project tsconfig.esm.json && tsc --skipLibCheck --project tsconfig.cjs.json",
    "release-package": "npm run build:tsc && npx changeset publish"
  },
  "author": "Marinescu Alexandru",
  "license": "MIT",
  "dependencies": {
    "@emotion/react": "11.11.3",
    "@emotion/styled": "11.11.0",
    "@mui/material": "5.15.0"
  },
  "peerDependencies": {
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.1",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "rimraf": "^6.1.2",
    "rollup": "^4.13.0",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-typescript2": "^0.36.0",
    "typescript": "^5.4.2"
  }
}
